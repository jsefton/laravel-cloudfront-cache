  "keywords": ["laravel","php","cache","aws", "cloudfront", "headers"],
  "homepage": "https://github.com/jsefton/laravel-cloudfront-cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamie Sefton",
      "email": "info@jamie-sefton.co.uk"
    }
  ],
  "require": {
    "laravel/framework": ">=5.4"
  },
  "archive": {
    "exclude": [
      "/tests",
      "tests",
      "tests/*.*",
      "phpunit.xml",
      ".gitignore",
      ".gitattributes",
      "README.md"
    ]
  },
  "autoload": {
    "psr-4": {
      "JSefton\\CloudFrontCache\\": "src/"
    }
  }
